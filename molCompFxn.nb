Cell[TextData[{
 "- ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["V", "i"], TraditionalForm]],ExpressionUUID->
  "57e9d670-bc26-4d4f-a2ca-10f5d8042319"],
 " defines the # of valence electrons according to groups of periodic table\n\
- ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["b", "i"], TraditionalForm]],ExpressionUUID->
  "0d075704-5269-407e-8046-4d44e7c03747"],
 " is the total # of bonds from C or other heteroatom to any other atom other \
than hydrogen \n- ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["d", "i"], TraditionalForm]],ExpressionUUID->
  "db34e74e-effa-4a12-a601-40823958db32"],
 " is the # of chemically nonequivalent bond to positions with ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["V", "i"], 
    SubscriptBox["b", "i"]}], TraditionalForm]],ExpressionUUID->
  "e53c326d-e16a-4c5a-8836-58a0fa700202"],
 " > 1\n- ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["e", "i"], TraditionalForm]],ExpressionUUID->
  "7309b425-448e-400f-ae94-d4ec634f8280"],
 " is the # of different elements or isotopes in the atom\[CloseCurlyQuote]s \
microenvironment \n- ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["s", "i"], TraditionalForm]],ExpressionUUID->
  "67f5c453-ad7a-4ae3-9ddb-5ceff9a338f3"],
 " is the # of isomeric possibilities at a given position "
}], "Text",
